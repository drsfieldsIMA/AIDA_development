{
    "env": {
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "google",
        "next"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react",  "next"
    ],
    "rules": {
      "react/no-unescaped-entities": "off",
      "@next/next/no-page-custom-font": "off"
    }
}
